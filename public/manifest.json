{
  "manifest_version": 3,

  "name": "chrome-extension-cra-material-ui",
  "description": "This Extension shows you an example Application which shows your current url on a React-App based on cra and material-ui.",
  "version": "0.0.1",

  "action": {
    "default_popup": "index.html",
    "default_title": "Open this Extension"
  },
  "icons": {
    "16": "logo-16.png",
    "48": "logo-64.png",
    "128": "logo-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "tabs"
    ],

    "host_permissions":[
      
    ]
}